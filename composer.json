    "minimum-stability": "stable",

    "type": "library",

    "keywords": [
        "sql",
        "table",
        "create",
        "mysql",
        "sqlite"
    ],
    "license": "MIT",

    "homepage": "https://github.com/Zrny/MkSQL",

    "authors": [
        {
            "name": "Štěpán Zrník",
